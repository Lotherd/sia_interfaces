<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="ImportDS"
		transaction-type="JTA">
		<jta-data-source>ImportDS</jta-data-source>
		<class>trax.aero.model.TaskCard</class>
		<class>trax.aero.model.TaskCardItem</class>
		<class>trax.aero.model.InterfaceLockMaster</class>
		<class>trax.aero.model.TaskCardItemPK</class>
		<class>trax.aero.model.TaskCardControlMaster</class>
		<class>trax.aero.model.TaskCardControlMasterPK</class>
		<class>trax.aero.model.TaskCardEffectivity</class>
		<class>trax.aero.model.TaskCardEffectivityPK</class>
		<class>trax.aero.model.TaskCardPn</class>
		<class>trax.aero.model.TaskCardPnPK</class>
		<class>trax.aero.model.TaskCardControl</class>
		<class>trax.aero.model.TaskCardControlPK</class>
		<class>trax.aero.model.PnMaster</class>
		<class>trax.aero.model.AcMaster</class>
		<class>trax.aero.model.SystemTranCode</class>
		<class>trax.aero.model.SystemTranCodePK</class>
		<class>trax.aero.model.TaskCardEffectivityHead</class>
		<class>trax.aero.model.TaskCardEffectivityHeadPK</class>
		<class>trax.aero.model.ProfileMaster</class>
		<class>trax.aero.model.TaskCardControlRev</class>
		<class>trax.aero.model.TaskCardControlRevPK</class>
		<class>trax.aero.model.TaskCardEffectivityHeadRv</class>
		<class>trax.aero.model.TaskCardEffectivityHeadRvPK</class>
		<class>trax.aero.model.TaskCardEffectivityRev</class>
		<class>trax.aero.model.TaskCardEffectivityRevPK</class>
		<class>trax.aero.model.TaskCardEffectivityRevHd</class>
		<class>trax.aero.model.TaskCardEffectivityRevHdPK</class>
		<class>trax.aero.model.TaskCardItemRev</class>
		<class>trax.aero.model.TaskCardItemRevPK</class>
		<class>trax.aero.model.TaskCardPnRev</class>
		<class>trax.aero.model.TaskCardPnRevPK</class>
		<class>trax.aero.model.TaskCardRev</class>
		<class>trax.aero.model.TaskCardRevPK</class>
		<class>trax.aero.model.AcTypeSeriesMaster</class>
		<class>trax.aero.model.AcTypeSeriesMasterPK</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="hibernate.dialect"
				value="org.hibernate.dialect.Oracle10gDialect" />
		</properties>
	</persistence-unit>
</persistence>
